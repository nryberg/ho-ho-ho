%p= "X = " + @x
%p= "Y = " + @y
%p= "Count " + @results.count.to_s
%p= @cols.join(", ")
%p= @rows.join(", ")
%table
  %th
    - @cols.each do |col|
      %td= col
  - @rows.each do |row|
    %tr
      %td= row
      - @cols.each do |col|
        %td= col
   
- @results.each do |result|
  %p= result["_id"]["city"]
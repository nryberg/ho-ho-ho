%p= "X = " + @x
%p= "Y = " + @y
%p= "Count " + @results.count.to_s
%p= @cols.join(", ")
%p= @rows.join(", ")
%p= @results.keys.join(", ")
- @results.keys.each do |key|
  %p= key + " " + @results[key]
%table
  %th
    - @cols.each do |col|
      %td= col
  - @rows.each do |@row|
    %tr
      %td= @row
      - @cols.each do |col|
        %td= @row.class
        / %td= @results[@row.to_s + col.to_s]